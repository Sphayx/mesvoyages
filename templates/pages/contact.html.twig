{%  extends "basefront.html.twig" %}

{% block body %}
    {{ form_start(formcontact)}}
    {% for message in app.flashes('sucess') %}
        <div class="mt-3 alert alert-sucess">
            {{ message }}
        </div>
    {% endfor %}
    <div class="row mt-3">
         <div class="col">
            {{ form_row(formcontact.nom) }}
        </div>
        <div class="col">
            {{ form_row(formcontact.email) }}
        </div>
    </div>
        <div class="row">
            <div class="col">
                {{ form_row(formcontact.message) }}
            </div>
        </div>
    {{ form_end(formcontact) }} 
{% endblock %}        